{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users/(.*)/role",
      "dest": "/api/users/[username]/role.js"
    },
    {
      "src": "/api/tournaments/(.*)/finish",
      "dest": "/api/tournaments/finish.js"
    },
    {
      "src": "/api/tournaments/(.*)/resolve-tie",
      "dest": "/api/tournaments/tie.js"
    },
    {
      "src": "/api/tournaments/(.*)",
      "dest": "/api/tournaments/[id].js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}